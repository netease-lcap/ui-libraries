            "parentAccept": "['u-table-view', 'u-table-view-column-group'].includes(target.tag)",
            "selector": [
                {
                    "expression": "this.getElement(el => el.slotTarget === 'title')",
                    "cssSelector": "th"
                },
                {
                    "expression": "this.getElement(el => el.slotTarget === 'cell')",
